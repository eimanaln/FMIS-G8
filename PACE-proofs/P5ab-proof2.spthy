     ]
    variants (modulo AC)
    1. $p    = $p.21
       s     = s.28
       y     = one
       z     = map(s.28, $p.21)
    
    2. $p    = $p.22
       s     = s.29
       y     = y.30
       z     = map(s.29, $p.22)^y.30
  */

rule (modulo E) A_4_recv:
   [
   St_A_4( A, B, idA, k, x, s, $p, gy, kab ),
   In( <'MAC_X', mac(<'R', map(s, $p)^x>, kab)> )
   ]
  --[
  Commit( A, B, <'I', 'R', map(s, $p)^x, gy, kab> ),
  Secret( A, B, kab ), Finish( A, B, 'I', map(s, $p)^x, gy, kab )
  ]->
   [ St_A_5( A, B, idA, k, x, s, $p, gy, kab ) ]

  /*
  rule (modulo AC) A_4_recv:
     [
     St_A_4( A, B, idA, k, x, s, $p, gy, kab ),
     In( <'MAC_X', mac(<'R', z>, kab)> )
     ]
    --[
    Commit( A, B, <'I', 'R', z, gy, kab> ), Secret( A, B, kab ),
    Finish( A, B, 'I', z, gy, kab )
    ]->
     [ St_A_5( A, B, idA, k, x, s, $p, gy, kab ) ]
    variants (modulo AC)
    1. $p    = $p.21
       s     = s.28
       x     = one
       z     = map(s.28, $p.21)
    
    2. $p    = $p.22
       s     = s.29
       x     = x.30
       z     = map(s.29, $p.22)^x.30
  */

lemma executable:
  exists-trace
  "∃ A B gx gy kab #i #j.
    (((Finish( B, A, 'R', gx, gy, kab ) @ #i) ∧
      (Finish( A, B, 'I', gx, gy, kab ) @ #j)) ∧
     (¬(∃ R A.1 B.1 id1 id2 #i.1 #j.1.
         ((Init( R, A.1, id1 ) @ #i.1) ∧ (Init( R, B.1, id2 ) @ #j.1)) ∧
         (¬(#i.1 = #j.1))))) ∧
    (¬(∃ X #k. Reveal( X ) @ #k))"
/*
guarded formula characterizing all satisfying traces:
"∃ A B gx gy kab #i #j.
  (Finish( B, A, 'R', gx, gy, kab ) @ #i) ∧
  (Finish( A, B, 'I', gx, gy, kab ) @ #j)
 ∧
  (∀ R A.1 B.1 id1 id2 #i.1 #j.1.
    (Init( R, A.1, id1 ) @ #i.1) ∧ (Init( R, B.1, id2 ) @ #j.1)
   ⇒
    #i.1 = #j.1) ∧
  (∀ X #k. (Reveal( X ) @ #k) ⇒ ⊥)"
*/
simplify
solve( Finish( B, A, 'R', gx, gy, kab ) @ #i )
  case B_4_send
  solve( St_B_4( B, A, idB, k, s, $p, gx, y, kab ) ▶₀ #i )
    case B_3_recv
    solve( Finish( $A, $B, 'I', gx, map(s, $p)^~y, h(z) ) @ #j )
      case A_4_recv
      solve( St_A_4( $A, $B, idA, k.1, x, s.1, $p.1, map(s, $p)^~y, h(z)
             ) ▶₀ #j )
        case A_3_send
        solve( !KU( senc(~s, h(~k)) ) @ #vk.6 )
          case A_1_send
          solve( !KU( mac(<'I', map(~s, $p)^~y>, h(map(~s, $p)^(~x*~y)))
                 ) @ #vk.3 )
            case A_3_send
            solve( !KU( mac(<'R', map(~s, $p)^~x>, h(map(~s, $p)^(~x*~y)))
                   ) @ #vk.12 )
              case B_4_send
              solve( !KU( map(~s, $p)^~y ) @ #vk.15 )
                case B_2_send
                solve( !KU( map(~s, $p)^~x ) @ #vk.11 )
                  case A_1_send
                  SOLVED // trace found
                qed
              qed
            qed
          qed
        qed
      qed
    qed
  qed
qed

